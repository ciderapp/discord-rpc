{
    "name": "@xhayper/discord-rpc",
    "version": "1.0.5",
    "description": "a fork of discordjs/RPC",
    "main": "dist/index.js",
    "author": "xhayper",
    "license": "ISC",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xhayper/discord-rpc.git"
    },
    "keywords": [
        "typescript",
        "discord",
        "ipc",
        "rpc",
        "rich-presence",
        "discord-rpc",
        "discord-ipc"
