        "@types/ws": "^8.5.3",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.3"
    },
    "engines": {
        "node": ">=14.17.0"
    },
    "packageManager": "yarn@3.2.3"
